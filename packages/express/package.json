{
  "name": "@subauth/express",
  "version": "0.0.1",
  "description": "Express integration for SubAuth - minimal setup authentication and subscriptions",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@subauth/core": "workspace:*",
    "@subauth/backend": "workspace:*",
    "@subauth/adapter-prisma": "workspace:*",
    "@subauth/adapter-ses": "workspace:*",
    "@subauth/adapter-paddle": "workspace:*",
    "jsonwebtoken": "^9.0.0"
  },
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0",
    "@prisma/client": "^5.0.0 || ^6.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.0",
    "typescript": "^5.3.0"
  }
}